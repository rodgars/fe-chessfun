{
    "parser": "@typescript-eslint/parser",
    "plugins": ["react"],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      // add your rules here
      "no-unused-vars": "warn",
      "quotes": ["error", "single"],
      "indent": ["error", 2],
      "react/jsx-uses-vars": "error"
    }
  }
  